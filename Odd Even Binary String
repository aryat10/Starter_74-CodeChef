#include <iostream>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        int a;
        cin >>a;
        int count = 0;
        for(int i=0;i<a;i++)
        {
            int m;
            cin >> m;
            if(m==0)
           { 
            count++;
           }
        }
        if(count%2==0)
        {
            cout <<"YES" << endl;
        }
        else{
            cout << "NO" << endl;
        }
    }
    return 0;
}

